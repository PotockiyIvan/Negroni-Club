@model IQueryable<Dish>

@{
    string strTitle = "Панель администратора";
    ViewBag.Title = strTitle;
}

<div id="banner" class="banner full-screen-mode parallax">
    <div class="container pr">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="banner-static">
                <div class="banner-text">

                    <div class="banner-cell">

                        <h1>@strTitle</h1>
                        <div class="div-box">
                            <h2>Редактировать блоки сайта</h2>
                            <a asp-area="Admin" asp-controller="Home" asp-action="EditBanner" asp-route-codeWord="Banner">Баннер</a>
                            <a asp-area="Admin" asp-controller="Home" asp-action="EditAboutUs" asp-route-codeWord="AboutUs">О нас</a>
                            <a asp-area="Admin" asp-controller="Home" asp-action="EditAboutUs" asp-route-codeWord="AboutUs">События</a>
                            <a asp-area="Admin" asp-controller="Dishes" asp-action="Index">Редактировать Меню</a>
                            @*<a asp-area="Admin" asp-controller="TextFields" asp-action="Edit" asp-route-codeWord="OrderHome">Закажи Домой</a>*@
                            <a asp-area="Admin" asp-controller="Teammate" asp-action="EditOurTeam">Наша Команда</a>
                            <a asp-area="Admin" asp-controller="Gallery" asp-action="Index">Галерея</a>
                            @*<a asp-area="Admin" asp-controller="TextFields" asp-action="Edit" asp-route-codeWord="Blog">Блог</a>*@
                            @*<a asp-area="Admin" asp-controller="TextFields" asp-action="Edit" asp-route-codeWord="Footer">Футер</a>*@
                            <a asp-area="" asp-controller="Home" asp-action="Index">На главную</a>
                        </div>
                        <div class="div-box">
                            <form asp-area="" asp-controller="Account" asp-action="Logout" method="post">
                                <input type="submit" value="Выйти" />
                            </form>
                        </div>
                    </div>
                    <!-- end banner-cell -->
                </div>
                <!-- end banner-text -->
            </div>
            <!-- end banner-static -->
        </div>
        <!-- end col -->
    </div>
    <!-- end container -->
</div>
<!-- end banner -->
